@model IEnumerable<CmsShoppingCart.Models.ViewModels.Pages.PageVM>

@{
    string tempPageTitle = (string)TempData["PageTitle"];
}


@foreach (var item in Model)
{
    if (tempPageTitle == item.Title)
    {
        <li style="color:blue; font-size: 20px;" class="active"><a href="/@item.Slug">@item.Title</a></li>

    }
    else
    {
        <li> <a style="color:blue; font-size: 20px;" href="/@item.Slug">@item.Title</a></li>
    }

}

